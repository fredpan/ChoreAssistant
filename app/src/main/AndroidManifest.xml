<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seg2105.doooge.choreassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.WelcomePageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.Chore.ChoreEdit"></activity>
        <activity android:name=".View.Chore.ChoreDetail"></activity>
        <activity android:name=".View.Responsibility.ResponsibilityList"></activity>
        <activity android:name=".View.User.ControlPanelActivity"></activity>
        <activity android:name=".View.User.RegistrationActivity"></activity>
        <activity android:name=".View.User.EditUserActivity"></activity>
        <activity android:name=".View.Reward.RewardEdit"></activity>

    </application>

</manifest>